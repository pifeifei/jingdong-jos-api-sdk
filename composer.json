{
    "name" : "jos/aces-php-sdk",
    "description": "aces php sdk",
    "type" : "library",
    "version": "1.0.0",
    "config" : {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload" : {
        "psr-4" : {
            "ACES\\": "src"
        }
    },
    "require" : {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "monolog/monolog": "^1.24.0|^2.5",
        "netresearch/jsonmapper": "^1.6.0|^2.0|^3.0|^4.0"
    },
    "require-dev": {
        "symfony/finder": "^5.4|^6.0",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "suggests": {
        "ext-mysqli": "链接MySQL用",
        "ext-sqlite": "sqlite3"
    }
}
